#include<iostream>
using namespace std;
//call by value
void fun(int no)
{
    cout<<"inside call by value:"<<no<<"\n";
    no++;


}
//call by address
void gun(int *p)
{
cout<<"inside call by address :"<<*p<<"\n";
(*p)++;

}
//call by referance
void sun(int &ref)
{
cout<<"value call by reference :"<<ref<<"\n"
ref++;

}
int main()
{

auto int i=10;
auto int j=10;
auto int k=10;
fun(i);   //fun(10)
cout<<i<<"\n";
gun(&j); //gun(200)
cout<<j<<"\n";
sunk(k);// sun(k);
cout<<k<<"\n";

 return 0;

}